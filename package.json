{
  "name": "strapped-wordpress-starter",
  "version": "2.0.0",
  "description": "Wordpress Starter Theme initially based on Underscores, Bootstrap 4 (with Sass), Webpack 4.",
  "author": "JPOak",
  "repository": {
    "type": "git",
    "url": "https://github.com/JPOak/strapped-wordpress-starter.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && webpack --mode development --watch --progress --config src/build/dev.config.js",
    "dev": "npm run clean && webpack --progress --mode development  --config src/build/dev.config.js",
    "build": "webpack --progress --mode production  --config src/build/prod.config.js",
    "clean": "rimraf dist/manifest.json dist/styles/*"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "3.1.1",
    "cssnano": "^5.0.9",
    "file-exists": "^5.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss-loader": "^6.2.0",
    "remove-files-webpack-plugin": "^1.4.5",
    "rimraf": "^3.0.2",
    "sass": "1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.61.0",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  }
}
